schema {
    query: Query
    mutation: Mutation
}

type Query {
    findAllCustomers: [Customer!]!
    findAllApps: [App!]!
    findAppById(appId: Int!): App
    findAllAppSubscriptions(customerId: String!, status: Int): [AppSubscription!]!
    findAllAgents: [Agent!]!
}

type Mutation {
    saveAppSubscription(input: CreateAppSubscriptionInput): AppSubscription!
}